{
  "manifest_version": 3,
  "name": "YouCineTV Chrome Extension",
  "version": "1.0",
  "description": "Tracks video and episode progress on YouCineTV.",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.youcinetv.vip/vod/details/*"
      ],
      "js": [
        "application/index.js"
      ],
      "type": "module"
    }
  ]
}
